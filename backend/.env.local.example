# Database Configuration
DATABASE_URL=sqlite:///./citycare.db
 
# JWT Configuration
SECRET_KEY=your-secret-key-change-in-production
 
# Admin API Key for bot integration
ADMIN_API_KEYS=<api-key-1>,<api-key-2>,<api-key-3>,<api-key-4>
 
# CORS Origins
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
 
# File Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760  # 10MB

 
# WatsonX Configuration
WATSONX_APIKEY=<api-key>
WATSONX_URL=insert-watsonx-url-here
PROJECT_ID=<project-id>
WATSONX_MODEL=ibm/modle-name